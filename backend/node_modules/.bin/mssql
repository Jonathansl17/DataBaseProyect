#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jonat/OneDrive - Estudiantes ITCR/3Semestre/BasesDeDatos/Proyecto1/DataBaseProyect/backend/node_modules/.pnpm/mssql@11.0.1/node_modules/mssql/bin/node_modules:/mnt/c/Users/jonat/OneDrive - Estudiantes ITCR/3Semestre/BasesDeDatos/Proyecto1/DataBaseProyect/backend/node_modules/.pnpm/mssql@11.0.1/node_modules/mssql/node_modules:/mnt/c/Users/jonat/OneDrive - Estudiantes ITCR/3Semestre/BasesDeDatos/Proyecto1/DataBaseProyect/backend/node_modules/.pnpm/mssql@11.0.1/node_modules:/mnt/c/Users/jonat/OneDrive - Estudiantes ITCR/3Semestre/BasesDeDatos/Proyecto1/DataBaseProyect/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/jonat/OneDrive - Estudiantes ITCR/3Semestre/BasesDeDatos/Proyecto1/DataBaseProyect/backend/node_modules/.pnpm/mssql@11.0.1/node_modules/mssql/bin/node_modules:/mnt/c/Users/jonat/OneDrive - Estudiantes ITCR/3Semestre/BasesDeDatos/Proyecto1/DataBaseProyect/backend/node_modules/.pnpm/mssql@11.0.1/node_modules/mssql/node_modules:/mnt/c/Users/jonat/OneDrive - Estudiantes ITCR/3Semestre/BasesDeDatos/Proyecto1/DataBaseProyect/backend/node_modules/.pnpm/mssql@11.0.1/node_modules:/mnt/c/Users/jonat/OneDrive - Estudiantes ITCR/3Semestre/BasesDeDatos/Proyecto1/DataBaseProyect/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/mssql@11.0.1/node_modules/mssql/bin/mssql" "$@"
else
  exec node  "$basedir/../.pnpm/mssql@11.0.1/node_modules/mssql/bin/mssql" "$@"
fi
